<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo_4"
    tools:context=".TelaPoliticaPrivacidade">

    <TextView
        android:id="@+id/lblDadosColetados"
        android:layout_width="332dp"
        android:layout_height="88dp"
        android:fontFamily="@font/montserrat_medium"
        android:text="@string/lblDadosColetados"
        android:textAlignment="textStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="238dp" />

    <TextView
        android:id="@+id/subtituloDadosColetados"
        android:layout_width="278dp"
        android:layout_height="38dp"
        android:fontFamily="@font/montserrat_medium"
        android:text="@string/subtituloDadosColetados"
        android:textAlignment="textStart"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="172dp" />

    <ImageView
        android:id="@+id/imgVoltarPoliticaPrivac"
        android:layout_width="50dp"
        android:layout_height="37dp"
        app:srcCompat="@drawable/voltar"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="31dp" />

    <TextView
        android:id="@+id/tituloPoliticaPrivac"
        android:layout_width="278dp"
        android:layout_height="38dp"
        android:fontFamily="@font/montserrat_medium"
        android:text="@string/tituloPoliticaPrivac"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="76dp" />

</androidx.constraintlayout.widget.ConstraintLayout>